{
  "name": "date-changer",
  "version": "0.1.0",
  "author": "Corey Lanier",
  "private": true,
  "scripts": {
    "client": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "server": "nodemon api/_server.js",
    "all": "concurrently --kill-others \"nodemon api/_server.js\" \"vue-cli-service serve\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^3.0.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "core-js": "^3.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.11.1",
    "express": "^4.17.1",
    "http": "0.0.0",
    "http-proxy-middleware": "^0.20.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.22.0",
    "mongoose": "^5.8.11",
    "vue": "^2.6.11",
    "vue-csv-import": "^3.1.0",
    "vue-datetime": "^1.0.0-beta.11",
    "vue-router": "^3.1.3",
    "vue-select": "^3.4.0",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "vue": {
    "devServer": {
      "proxy": {
        "^/api": {
          "target": "http://localhost:4000",
          "ws": true,
          "changeOrigin": true
        }
      }
    }
  },
  "now": {
    "alias": "date-setter",
    "name": "icad-dubai-dates",
    "env": {
      "PORT": "4000",
      "SECRET": "@secret_icad",
      "MONGODB_URI": "@mongodb_uri_icad"
    },
    "routes": [
      {
        "src": "/",
        "dest": "index.html"
      },
      {
        "src": "/api/modules/(.+)",
        "dest": "/api/modules.js"
      },
      {
        "src": "/api/users/(.+)",
        "dest": "/api/users.js"
      },
      {
        "src": "/api/teachers/(.+)",
        "dest": "/api/teachers.js"
      },
      {
        "src": "/api/assignments/(.+)",
        "dest": "/api/assignments.js"
      },
      {
        "src": "/api/courses/(.+)",
        "dest": "/api/courses.js"
      },
      {
        "src": "/api/students/(.+)",
        "dest": "/api/students.js"
      },
      {
        "src": "/api/items/(.+)",
        "dest": "/api/items.js"
      }
    ]
  }
}
